Correspondence of Array




#include<stdio.h>
#include<conio.h>
int main ()
{
    int a[10],b[10],c[10],i;
    printf("Enter the first array element: \n");
    for (i = 0; i < 10; i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the second array element: \n");
    for (i = 0; i < 10; i++)
    {
        scanf("%d",&b[i]);
    }
    for ( i = 0; i < 10; i++)
    {
        c[i] = a[i] + b[i];   
    }
    printf("The correspondence of the array element: \n");
    for (i = 0; i < 20; i++)
    {
        printf("%d  ",c[i]);
    }

    return 0;
       
}
